{
    "name": "Mach3Script",
    "displayName": "Mach3Script",
    "description": "Mach3 Script Language Support",
    "version": "0.1.0",
    "publisher": "CalDymos",
    "license": "MIT",
    "icon": "resources/m1s.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/CalDymos/vscode-Mach3Script.git"
    },
    "bugs": {
        "url": "https://github.com/CalDymos/vscode-Mach3Script/issues"
    },
    "homepage": "https://github.com/CalDymos/vscode-Mach3Script/blob/master/README.md",
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Languages",
        "Snippets"
    ],
    "keywords": [
        "M1S",
        "Mach3",
        "Cypress Enable",
        "Script",
        "Language"
    ],
    "contributes": {
        "languages": [
            {
                "id": "m1s",
                "aliases": [
                    "Mach3Script",
                    "m1s"
                ],
                "extensions": [
                    ".m1s"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "m1s",
                "scopeName": "source.m1s",
                "path": "./syntaxes/m1s.tmLanguage.json"
            }
        ],
        "snippets": [
            {
                "language": "m1s",
                "path": "./snippets/m1s.json"
            }
        ]
    }
}
